<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
       http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">
        <!--操作日志 -->
    <!--dao -->
    <bean id="contactDAO" class="cn.com.do1.component.contact.contact.dao.impl.ContactDAOImpl" parent="baseDAO" />
    <bean id="contactReadOnlyDAO" class="cn.com.do1.component.contact.contact.dao.impl.ContactReadOnlyDAOImpl" parent="readonlySqlBaseDAO" />
    <bean id="selectUserReadOnlyDAO" class="cn.com.do1.component.contact.contact.dao.impl.SelectUserReadOnlyDAOImpl" parent="readonlySqlBaseDAO" />
    <bean id="contactInsideReadOnlyDAO" class="cn.com.do1.component.contact.contact.dao.impl.ContactInsideReadOnlyDAOImpl" parent="readonlySqlBaseDAO" />
    <bean id="contactCustomDAO" class="cn.com.do1.component.contact.contact.dao.impl.ContactCustomDAOImpl" parent="baseDAO" ></bean>
    <!-- service -->
    <context:component-scan base-package="cn.com.do1.component.contact.contact.service.impl" />
    <!-- action -->
    <context:component-scan base-package="cn.com.do1.component.contact.contact.ui" />

</beans>

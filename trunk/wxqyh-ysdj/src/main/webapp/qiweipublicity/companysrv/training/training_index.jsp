<script>
    $(function(){
        var header = $("#header");
        $(window).bind("scroll",function() {
            if($(window).scrollTop()>450){
                $("#header").stop().animate({top:0},500);
            }else{
                $("#header").stop().animate({top:-70+"px"},500);
            }
        });
    });
</script>
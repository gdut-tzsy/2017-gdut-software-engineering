/*评论头像弹出框*/
.personalBox{width:270px;margin:0 auto;top:45%;position:absolute;left:50%;background:#fff;text-align:center;z-index:1111;line-height:1.5;border-radius:5px;border-radius:5px;transform:translate(-50%,-50%);-webkit-transform:translate(-50%,-50%);}

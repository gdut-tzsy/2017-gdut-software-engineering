/* loading */
@-webkit-keyframes fade{
    from{opacity:1;}
    to{opacity:0.1;}
}
div.loading{}
div.loading div{position:relative;width:30px;height:30px;margin:0 auto;}
div.loading div span{width:10%;height:30%;position:absolute;top:40%;left:45%;opacity:0;background:#fff;-webkit-animation:fade 1s linear infinite;-webkit-border-radius:10px;}
div.loading div span:nth-child(1){-webkit-transform:rotate(0deg) translate(0,-142%);-webkit-animation-delay:0s;}
div.loading div span:nth-child(2){-webkit-transform:rotate(30deg) translate(0,-142%);-webkit-animation-delay:-0.9167s;}
div.loading div span:nth-child(3){-webkit-transform:rotate(60deg) translate(0,-142%);-webkit-animation-delay:-0.833s;}
div.loading div span:nth-child(4){-webkit-transform:rotate(90deg) translate(0,-142%);-webkit-animation-delay:-0.75s;}
div.loading div span:nth-child(5){-webkit-transform:rotate(120deg) translate(0,-142%);-webkit-animation-delay:-0.667s;}
div.loading div span:nth-child(6){-webkit-transform:rotate(150deg) translate(0,-142%);-webkit-animation-delay:-0.5833s;}
div.loading div span:nth-child(7){-webkit-transform:rotate(180deg) translate(0,-142%);-webkit-animation-delay:-0.5s;}
div.loading div span:nth-child(8){-webkit-transform:rotate(210deg) translate(0,-142%);-webkit-animation-delay:-0.41667s;}
div.loading div span:nth-child(9){-webkit-transform:rotate(240deg) translate(0,-142%);-webkit-animation-delay:-0.333s;}
div.loading div span:nth-child(10){-webkit-transform:rotate(270deg) translate(0,-142%);-webkit-animation-delay:-0.25s;}
div.loading div span:nth-child(11){-webkit-transform:rotate(300deg) translate(0,-142%);-webkit-animation-delay:-0.1667s;}
div.loading div span:nth-child(12){-webkit-transform:rotate(330deg) translate(0,-142%);-webkit-animation-delay:-0.0833s;}


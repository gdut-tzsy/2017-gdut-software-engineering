.fujian .TXT{background:url('../../../jsp/wap/images/txt.png') no-repeat;background-size:100%;-webkit-background-size:100%;}
.fujian .XLS,.fujian .XLSX{background:url('../../../jsp/wap/images/excel.png') no-repeat;background-size:100%;-webkit-background-size:100%;}
.fujian .FILE{background:url('../../../jsp/wap/images/file.png') no-repeat;background-size:100%;-webkit-background-size:100%;}
.fujian .MP4{background:url('../../../jsp/wap/images/mp4.png') no-repeat;background-size:100%;-webkit-background-size:100%;}
.fujian .MP3,.fujian .WMA,.fujian .AMR{background:url('../../../jsp/wap/images/music.png') no-repeat;background-size:100%;-webkit-background-size:100%;}
.fujian .PDF{background:url('../../../jsp/wap/images/pdf.png') no-repeat;background-size:100%;-webkit-background-size:100%;}
.fujian .PPT,.fujian .PPTX{background:url('../../../jsp/wap/images/ppt.png') no-repeat;background-size:100%;-webkit-background-size:100%;}

.fujian_text{position:relative;}
.fujian .name{font-size:13px;font-weight:bold;}
.fujian_size{margin-top:5px;font-size:12px;}
.fujian .arrow{position:absolute;right:0;top:0;z-index:2;height:100%;line-height:40px;font-family:宋体;font-size:18px;font-weight:bold;}
/*高级搜索弹出层*/
@keyframes senior_search1 
{
0%   {transform:translateX(270px)} 
100% {transform:translateX(0)}
}
@-webkit-keyframes senior_search1 
{
0%   {-webkit-transform:translateX(270px)} 
100% {-webkit-transform:translateX(0)}
}
@keyframes senior_search2
{
0%   {transform:translateX(0)} 
100% {transform:translateX(270px)}
}
@-webkit-keyframes senior_search2
{
0%   {-webkit-transform:translateX(0)} 
100% {-webkit-transform:translateX(270px)}
}
.sideWrap { position: fixed; right: 0; top: 0; background-color: #fff; height: 100%; z-index: 1111;width: 290px;transform: translate3d(290px, 0px, 0px); -webkit-transform: translate3d(290px, 0px, 0px);transition:all 0.2s ease;-webkit-transition:all 0.2s ease;overflow:hidden;display:none;}
.sideWrap li { list-style: none; line-height: 36px; height: 36px; margin-top: 5px; overflow: hidden; box-sizing: border-box; }
.SS_head { height: 45px; line-height: 45px; font-size: 16px; border-bottom: 1px solid #ccc; }
.SS_colse { width: 45px; height: 100%; border-right: 1px solid #eee; display: block; text-align: center; float: left;cursor: pointer; }
.SS_title{ text-align: center}
.sideWrap ul{overflow-y:auto;overflow-x:hidden;}
.sideWrap li input{ height: 34px; padding: 0 5px; box-sizing: border-box; outline: none; border: 1px solid #ddd; width: 180px;-webkit-appearance: none;line-height:34px;border-radius:0;font-size:13px; }
.sideWrap li select{ height: 34px; padding: 0 5px; box-sizing: border-box; outline: none; border: 1px solid #ddd; width: 180px;-webkit-appearance: none;line-height:34px;border-radius:0;white-space: nowrap;font-size:13px; }
.sideWrap li select.sheng{width: 65px;float:left;padding:0 4px;}
.sideWrap li select.shi{width: 105px; margin-left:10px;padding:0 4px;}
.sideWrap li .date{  width: 80px;display: block;float: left;}
.sideWrap li span { display: block; width: 60px; float: left; font-size: 13px; text-align: right; padding-right: 10px; }
.sideWrap li span.zhi{float:left;width:20px;padding:0;text-align:center;}
.SS_main { padding: 0 15px; }
.SS_colse::after { background: url(../images/CSSSprites.png?1) no-repeat; content: ''; width: 13px; height: 13px; position: absolute; background-position: -30px 0; top: 15px; left: 15px; }
.senior_search_btn::after{ width: 18px; background: url(../images/CSSSprites.png?1) no-repeat; background-position: -27px 0; background-size: 100px 100px; height: 14px; content:''; position: absolute; left: 14px; top: 8px; }
.senior_search_btn { width: 35px; height: inherit; margin-left: 14px; line-height: 44px; position: relative; }
.senior_search_solid{ height: 100%; width: 1px; background-color: #fff; position: absolute; top: 0; right: 44px; -webkit-transform: scaleX(.3); transform: scaleX(0.3)}
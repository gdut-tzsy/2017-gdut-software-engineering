.personalBox .iconBox.flexbox::before{content:'';width:100%;height:1px;position:absolute;left:0;top:0;background:#ccc;transform:scaleY(.5);-webkit-transform:scaleY(.5);transform-origin:0 0;-webkit-transform-origin:0 0;}

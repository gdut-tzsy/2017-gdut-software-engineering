/*按钮*/
.ui_btn,
.medium_btn {
    display: inline-block;
    text-align: center;
    color: #fff;background-color: #31ce32;
    border-radius: 5px;
    background-clip: padding-box;
}
.ui_btn {height: 45px;
    font-size: 18px;
}
.medium_btn {height: 35px;padding-left: 10px;padding-right: 10px;min-width: 80px;
    border-radius:2px;font-size: 15px
}
.ui_btn[disabled],
.medium_btn[disabled] {background-color: #8bbbe5}

/*radio单选*/
.radio_check input[type="radio"]+i {display: inline-block;vertical-align: middle;margin-right: 5px;
    width: 15px;height: 15px;background-color: #fff;
    border: 1px solid #ccc;border-radius: 100%;background-clip: padding-box;
}
.radio_check input[type="radio"]:checked+i {
    background: url(../images/gou.png) no-repeat center #f7941d;
    background-size: 70%;
}

/*复选滑块*/
.slider_check {
    position: relative;display: inline-block;
}
.slider_check > span,
.slider_check > span > i {
    display: inline-block;
    vertical-align: middle;
}
.slider_check > span {
    width: 55px;
    height: 24px;
    padding: 2px 4px;
    background-color: #e9e9e9;
    border-radius: 12px;
    background-clip: padding-box;
}
.slider_check > span > i {
    position: absolute;
    top: 4px;
    left: 2px;
    z-index: 2;
    width: 18px;
    height: 18px;
    border: 1px solid #dedede;
    border-radius: 100%;
    background-clip: padding-box;
    background-image: -moz-linear-gradient(90deg, rgb(230, 230, 230) 0%, rgb(239, 239, 239) 31%, rgb(247, 247, 247) 100%);
    background-image: -webkit-linear-gradient(90deg, rgb(230, 230, 230) 0%, rgb(239, 239, 239) 31%, rgb(247, 247, 247) 100%);
    background-image: gradient(90deg, rgb(230, 230, 230) 0%, rgb(239, 239, 239) 31%, rgb(247, 247, 247) 100%);
    transition: left .3s ease;
    -webkit-transition: left .3s ease;
}
.slider_check > span > b.slider_no {
    position: absolute;
    right: 10px;
    top: 3px;
    color: #999;
}
.slider_check > span > b.slider_yes {
    position: absolute;
    left: 10px;
    top: 3px;
    color: transparent;
}
.slider_check > input[type="checkbox"]:checked + span {
    background-color: #76c5f0
}
.slider_check > input[type="checkbox"]:checked + span b.slider_yes{
    color: #fff;
}
.slider_check > input[type="checkbox"]:checked + span b.slider_no{
    color: transparent;
}
.slider_check > input[type="checkbox"]:checked + span > i {
    left: 38px;
}


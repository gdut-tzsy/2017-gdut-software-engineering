/*轮滑*/
.swiper-container {
    position: relative;
    overflow: hidden;
}
.boxModal {
    display: box;
    display: -webkit-box;
    display: -moz-box;
}
/*swiper分页器*/
.swiper-pagination-circle {
    position: absolute;
    bottom: 0;
    width: 100%;
    text-align: center;
}
.swiper-pagination-circle > span {
    display: inline-block;
    width: 8px;
    height: 8px;
    margin-right: 5px;
    background-color: #fff;
    opacity: 0.5;
    border-radius: 100%;
    background-clip: padding-box;
}
.swiper-pagination-circle > span.swiper-pagination-bullet-active,
.swiper-pagination-square > span.swiper-pagination-bullet-active {
    background-color: #12b7f5;
    opacity:.8;
}
/*==================微信弹出框==================*/
.weui_mask1 { position: fixed; z-index: 1300; width: 100%; height: 100%; top: 0; left: 0; background: rgba(0, 0, 0, 0.6); }
.weui_dialog1 { position: relative; z-index: 1301; width: 85%;/* top: 50%; 
    left: 50%; -webkit-transform: translate(-50%, -50%); transform: translate(-50%, -50%); */
    background-color: #FAFAFC; text-align: center; border-radius: 3px; font-size: 16px;opacity: 0;
    -webkit-transform: scale(1.185);
  transform: scale(1.185);
    -webkit-transition: all .3s;
  transition: all .3s;
}
.custom_dialog1 {position: fixed;
    left: 0;
    top: 0;
    z-index: 1500;
    width: 100%;
    height: 100%;
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -ms-flex-pack: center;
    -webkit-box-pack: center;
    box-pack: center;
    -webkit-justify-content: center;
    justify-content: center;
    -ms-flex-align: center;
     -webkit-box-align: center; 
    box-align: center;
    -webkit-align-items: center;
    align-items: center;
}
.weui_dialog_confirm1 .weui_dialog1 .weui_dialog_hd1 { padding: 1.2em 20px .5em; }
.weui_dialog_confirm1 .weui_dialog1 .weui_dialog_bd1 { text-align: left; }
.weui_dialog_hd1 { padding: 1.2em 0 .5em;}
.weui_dialog_title1 { font-weight: 400; font-size: 17px; }
.weui_dialog_bd1 { padding: 0 20px; font-size: 15px; color: #888; }
.weui_dialog_ft1 { position: relative; line-height: 42px; margin-top: 20px; font-size: 17px; display: -webkit-box; display: -webkit-flex; display: -ms-flexbox; display: flex; }
.weui_dialog_ft1 a { display: block; -webkit-box-flex: 1; -webkit-flex: 1; -ms-flex: 1; flex: 1; color: #3CC51F; text-decoration: none; -webkit-tap-highlight-color: rgba(0, 0, 0, 0); }
.weui_dialog_ft1 a:active { background-color: #EEEEEE; }
.weui_dialog_ft1:after { content: " "; position: absolute; left: 0; top: 0; width: 100%; height: 1px; border-top: 1px solid #D5D5D6; color: #D5D5D6; -webkit-transform-origin: 0 0; transform-origin: 0 0; -webkit-transform: scaleY(0.5); transform: scaleY(0.5); }
.weui_dialog_confirm1 .weui_dialog_ft1 a { position: relative; }
.weui_dialog_confirm1 .weui_dialog_ft1 a:after { content: " "; position: absolute; left: 0; top: 0; width: 1px; height: 100%; border-left: 1px solid #D5D5D6; color: #D5D5D6; -webkit-transform-origin: 0 0; transform-origin: 0 0; -webkit-transform: scaleX(0.5); transform: scaleX(0.5); }
.weui_dialog_confirm1 .weui_dialog_ft1 a:first-child:after { display: none; }
.weui_btn_dialog1.default1 { color: #353535; }
.weui_btn_dialog1.primary1 { color: #12b7f5; }
@media screen and (min-width: 1024px) {.weui_dialog1 { width: 35%;}}

.pop_in {
  -webkit-transform: scale(1);
  transform: scale(1);
  opacity: 1;
}

.pop_out {
  -webkit-transform: scale(0.815);
  transform: scale(0.815);
  opacity: 0;
}
/*tab*/
.ui_tab {border-bottom: 1px solid #e5e5e5;background-color: #fff}
.ui_tab>li{position: relative;text-align: center;}
/*.ui_tab>li:after {position: absolute;content: '';right:0;top:10px;height:25px;border-left: 1px solid #e5e5e5;}*/
.ui_tab>li a {/*display: inline-block;padding-left: 10px;padding-right: 10px;*/line-height: 40px;color: #666;font-size: 15px;}
.ui_tab>li.on a{color: #32cd32;/*border-bottom:3px solid #ff9b20;*/}
.ui_tab>li a.ib {height: 100%}
.ui_tab>li.on a.ib {border-bottom: 3px solid #00b3e2;}
.ui_tab+.swiper-scrollbar{margin-top: -3px}
.swiper-scrollbar-drag {height: 3px;background:#32cd32;}

.nav_tab_wrap {padding: 10px 20px}
.ui_nav_tab {border: 1px solid #00b3e2;border-radius: 5px;background-clip: padding-box;}
.ui_nav_tab>a {display: block;padding-top: 5px;padding-bottom: 5px;text-align: center;
  border-left: 1px solid #00b3e2;color: #00b3e2}
.ui_nav_tab>a:first-child {border-left: none;} 
.ui_nav_tab>a.on {background-color:#00b3e2;color: #fff}


